DEFINE_PATCH_FUNCTION ADD_SPELL_EFFECT_206	STR_VAR resource = ~~ // Ajoute une instruction "Protection contre le sort X" (HLAs du SDG)
BEGIN
	LPF ADD_SPELL_EFFECT
	INT_VAR
	opcode = 206		// cast spell at creature
	target = 2			// pre-target
	resist_dispel = 3	//resist = Dispel - Bypass resistance
	STR_VAR
	resource = EVAL ~%resource%~	// the spell to cast
	END
END

DEFINE_PATCH_FUNCTION ADD_SPELL_EFFECT_146	STR_VAR resource = ~~ // Ajoute une instruction "Lancer le sort X sur la cible"
BEGIN
	LPF ADD_SPELL_EFFECT
	INT_VAR
	opcode = 146		// cast spell at creature
	target = 2			// pre-target
	timing = 1			// permanent
	parameter2 = 1		// type = instant casting
	insert_point = 0	// insert as the first effect
	STR_VAR
	resource = EVAL ~%resource%~	// the spell to cast
	END
END

DEFINE_PATCH_FUNCTION CHANGE_TARGET // Change la cible de tous les effets de tous les niveaux d'un sort (HLAs du SDG)
BEGIN
	READ_LONG 0x64 abilities_off
	READ_SHORT 0x68 num_abilities
	READ_LONG 0x6a off_feat
	FOR (i = 0; i < num_abilities; i += 1) BEGIN
		WRITE_BYTE 0x0c+abilities_off+i*0x28 1 //Creature
		WRITE_BYTE 0x0e+abilities_off+i*0x28 30 //Range
		READ_SHORT 0x1e+abilities_off+i*0x28 nb_feat
		READ_SHORT 0x20+abilities_off+i*0x28 ind_feat
		FOR (p = 0; p < nb_feat; p += 1) BEGIN
			WRITE_BYTE 0x02+off_feat+p*0x30+ind_feat*0x30 2 //Pre target
		END
	END
END

